{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "outputDirectory": "build",
  "env": {
    "REACT_APP_NEWS_API_KEY": "9e76e457ea734bd79ae1f3b784796948"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
} 